{
  "name": "secure-db-proxy",
  "version": "1.0.0",
  "type": "module",
  "description": "A secure database proxy system with sender-receiver architecture for hospital data management",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "create-hash": "node create_hash.js",
    "setup-db": "echo 'Please run setup_database.sql manually in MySQL Workbench'",
    "clean": "rmdir /s /q node_modules && del package-lock.json",
    "init": "node -e \"import('http').then(http => http.get('http://localhost:3000/init-system', (res) => { console.log('Init response:', res.statusCode); })).catch(console.error)\""
  },
  "keywords": [
    "database",
    "proxy", 
    "security",
    "mysql",
    "hospital",
    "healthcare",
    "data-transfer",
    "authentication"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.6.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}